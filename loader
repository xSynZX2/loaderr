function sh()
    --server hop
    local goofyahhint = math.random(16, 26)
    for i = 1, 100 do
local Player = game.Players.LocalPlayer    
local Http = game:GetService("HttpService")
local TPS = game:GetService("TeleportService")
local Api = "https://games.roblox.com/v1/games/"

local _place,_id = game.PlaceId, game.JobId
local _servers = Api.._place.."/servers/Public?sortOrder=Desc&limit=100"
function ListServers(cursor)
   local Raw = game:HttpGet(_servers .. ((cursor and "&cursor="..cursor) or ""))
   return Http:JSONDecode(Raw)
end

local Next; repeat
   local Servers = ListServers(Next)
   for i,v in next, Servers.data do
       if tonumber(v.playing) < goofyahhint and v.id ~= _id then
           local s,r = pcall(TPS.TeleportToPlaceInstance,TPS,_place,v.id,Player)
           if s then break end
       end
   end
   
   Next = Servers.nextPageCursor
until not Next
end
--server hop
end

function collect()    
    local discovered_value = false

    if discovered_value ~= true then
        --not game.Workspace:FindFirstChild("GingerbreadCrate"):FindFirstChild("Umbrella1")
    if game.Workspace:FindFirstChild("GingerbreadCrate") then
        local cframevalue = game.Workspace:FindFirstChild("GingerbreadCrate"):GetPivot()
        local x = cframevalue.X
        local y = cframevalue.Y
        local z = cframevalue.Z
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(x, y, z) * CFrame.new(0, 10, 0)
        wait(2)
        if not game.Workspace:FindFirstChild("GingerbreadCrate"):FindFirstChild("Box") then
            wait(3)
        end
        
        game.Workspace:FindFirstChild("GingerbreadCrate").Box.Rotation = Vector3.new(0, 0, 0)
        local Box_ = game.Workspace:FindFirstChild("GingerbreadCrate"):FindFirstChild("Box")
        local goofyahh = 50
        for i = 1, 85 do
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Box_.CFrame * CFrame.new(0, goofyahh, 0)
        goofyahh = goofyahh - 0.5
        task.wait(0.02)
        end
        for i = 1, 100 do
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Box_.CFrame * CFrame.new(0, 8, 0)
            task.wait(0.02)
            end

        wait(2)
        local VirtualInputManager = game:GetService("VirtualInputManager")
        for i = 1, 2 do
        VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.E, false, game)
        task.wait(2)
        VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.E, false, game)
        task.wait(2)
        discovered_value = true
        end
    end
    end
    
end

repeat task.wait() until game:IsLoaded()
wait(1)

if game.Workspace:FindFirstChild("GingerbreadCrate") then
    
    print(game.Workspace:FindFirstChild("GingerbreadCrate").Name, "Found")
game:GetService("ReplicatedStorage").Remote.RemoteFunction:InvokeServer("RequestTeamChange", "Police")
wait(5)


    local hum = game.Players.LocalPlayer.Character.HumanoidRootPart
    hum.CFrame = CFrame.new(-4062, 22, 1307)
        wait(1)
        hum.CFrame = CFrame.new(-4004, 23, 1316)
        wait(2)
        hum.CFrame = CFrame.new(-3949, 23, 1323)
        wait(1)
        hum.CFrame = CFrame.new(-3949, 8000, 1323)
        wait(1)
        
    collect()
    
    game.Players.LocalPlayer:Kick("Mad City Auto Famr : Squid Hub  " ..sentance)
        sh()

    else
        print("Not found")
        game.Players.LocalPlayer:Kick("Mad City Auto Famr : Squid Hub  " ..sentance)
        sh()
end
